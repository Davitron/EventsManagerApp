    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "*",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "*",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.19",
    "cloudinary": "^1.9.1",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.9",
    "deep-equal": "^1.0.1",
    "dotenv": "^4.0.0",
    "expect": "^22.4.3",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.6",
    "hammerjs": "^2.0.8",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "is-date-format": "^0.1.1",
    "is-valid-date": "0.0.1",
    "jest-localstorage-mock": "^2.2.0",
    "jquery": "^3.3.1",
    "jquery.2": "^1.0.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "node-mailer": "^0.1.1",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "^0.0.3",
    "nodemailer": "^4.4.1",
    "nodemon": "^1.12.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "rc-pagination": "^1.16.3",
    "rc-select": "^7.7.8",
    "react": "^16.2.0",
    "react-alert": "^2.4.0",
    "react-animate-css": "^0.0.1",
    "react-calendar": "^2.13.5",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.2.0",
    "react-paginate": "^5.2.2",
    "react-redux": "^5.0.6",
    "react-redux-loading-bar": "^3.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.2.0",
    "react-tooltip": "^3.6.0",
