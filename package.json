{
  "name": "eventsmanagerapp",
  "version": "1.0.0",
  "description": "An Event Management Application for event centers and  event booking.",
  "main": "./server/server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build:server": "babel ./server --out-dir build -s",
    "load-config": "cp ./server/config/config.json ./build/config/config.json",
    "load-swagger": "mkdir ./build/doc && cp ./server/doc/swagger.json ./build/doc/swagger.json",
    "build": "npm run clean && npm run build:server && npm run load-config && npm run load-swagger",
    "test": " nyc --reporter=html --reporter=text mocha --timeout 10000 ./server/tests/* --exit --require babel-register --reporter spec",
    "babel-node": "babel-node --preset=es2015",
    "start": "node ./build/server.js",
    "build:client": "webpack --config webpack.config.js",
    "start:dev": "npm run webpack & nodemon --exec npm run babel-node -- ./server/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "test:dev": "npm run test && npm run coverage",
    "postinstall": "npm run build",
    "webpack": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davitron/EventsManagerApp.git"
  },
  "author": "OLUWASEGUN MATTHEWS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Davitron/EventsManagerApp/issues"
  },
  "homepage": "https://github.com/Davitron/EventsManagerApp#readme",
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
