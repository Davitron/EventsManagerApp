{
  "name": "eventsmanagerapp",
  "version": "1.0.0",
  "description": "An Event Management Application for event centers and  event booking.",
  "main": "./server/server.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .jsx --ext .js **",
    "lint-fix": "./node_modules/.bin/eslint --fix .jsx --ext .js **",
    "clean": "rm -rf build && mkdir build",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "babel ./server --out-dir build -s",
    "load-config": "cp ./server/config/config.json ./build/config/config.json",
    "load-swagger": "mkdir ./build/doc && cp ./server/doc/swagger.json ./build/doc/swagger.json",
    "full-build": "npm run build:server && npm run build:client && npm run load-config && npm run load-swagger",
    "heroku-postbuild": "npm run full-build",
    "db:migration": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "test:server": "NODE_ENV=test npm run db:migration && nyc --reporter=html --reporter=text mocha --timeout 10000 ./server/tests/* --exit --require babel-register --reporter spec",
    "test:client": "jest -u /client/src && codecov",
    "full-test": "npm run test:client && npm run test:server",
    "server:e2e": "NODE_ENV=test npm run db:migration && npm run build:client && NODE_ENV=test nodemon --exec npm run babel-node -- ./server/server.js",
    "test:e2e": "NODE_ENV=test ./node_modules/.bin/nightwatch",
    "babel-node": "babel-node --preset=es2015",
    "start": "node ./build/server.js",
    "start:dev": "npm run webpack & nodemon --exec npm run babel-node -- ./server/server.js",
    "webpack": "webpack --config webpack.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davitron/EventsManagerApp.git"
  },
  "author": "OLUWASEGUN MATTHEWS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Davitron/EventsManagerApp/issues"
  },
  "homepage": "https://github.com/Davitron/EventsManagerApp#readme",
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "e2etests/*"
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "compression-webpack-plugin": "^1.1.10",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-enzyme": "^4.2.0",
    "mocha": "^3.5.2",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.21",
    "nyc": "^11.3.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "selenium-standalone": "^6.15.0",
    "sinon": "^5.0.7",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "accounting-js": "^1.1.1",
    "animate.css": "^3.6.1",
    "append-query": "^2.0.1",
    "await": "^0.2.6",
    "axios": "^0.17.1",
