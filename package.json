{
  "name": "eventsmanagerapp",
  "version": "1.0.0",
  "description": "An Event Management Application for event centers and  event booking.",
  "main": "index.js",
  "scripts": {
    "prestart": "sequelize db:migrate && sequelize db:seed:all",
    "test": " nyc --reporter=html --reporter=text mocha ./server/tests/* --exit --require babel-register --reporter spec",
    "babel-node": "babel-node --preset=es2015",
    "start": "nodemon --exec npm run babel-node -- ./server/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "test:dev": "npm run test && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davitron/EventsManagerApp.git"
  },
  "author": "OLUWASEGUN MATTHEWS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Davitron/EventsManagerApp/issues"
  },
  "homepage": "https://github.com/Davitron/EventsManagerApp#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^3.5.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "is-date-format": "^0.1.1",
    "is-valid-date": "0.0.1",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.0.0",
    "uuid": "^3.1.0",
    "validatorjs": "^3.13.5"
  }
