    "redux-persist-cookie-storage": "^1.0.0-beta",
    "redux-storage": "^4.1.2",
    "redux-storage-engine-localstorage": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "sequelize": "^4.23.1",
    "sequelize-cli": "^3.0.0",
    "shortid": "^2.2.8",
    "style-loader": "^0.20.1",
    "swagger-ui": "^3.6.0",
    "swagger-ui-express": "^2.0.11",
    "toastify-js": "^1.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "universal-cookie": "^2.1.2",
    "url-loader": "^0.6.2",
    "uuid": "^3.1.0",
    "validatorjs": "^3.13.5",
    "webpack": "^4.3.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.19.1"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage/client/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!src/index.jsx",
      "client/src/**/*.{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "globals": {
      "window": true
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/mock-data/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ],
    "setupFiles": [
      "<rootDir>/client/src/setups/setupTests.js",
      "<rootDir>/client/src/setups/browserMock.js",
      "jest-localstorage-mock"
    ]
  }
}
